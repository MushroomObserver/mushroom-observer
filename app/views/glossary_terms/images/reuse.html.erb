<%
@title = :image_reuse_title.t(name: @object.unique_format_name)

tabs = create_tabs(glossary_term_image_form_links(term: @object))
@tabsets = { right: draw_tab_set(tabs) }
@container = :full

form_action = { controller: "/glossary_terms/images",
                action: :attach,
                id: @object.id,
                q: get_query_param }
%>

<%= render(partial: "shared/images/images_to_reuse",
           locals: { form_action: form_action }) %>
