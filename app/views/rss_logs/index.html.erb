<%
  @title = content_tag(:span, :rss_log_title.t, class: "text-nowrap")
  tabs = create_tabs(rss_logs_index_links(user: @user, types: @types))
  @tabsets = { right: draw_tab_set(tabs) }
  @full_width_tab_set = render(partial: "type_filters")

  @container = :full

  flash_error(@error) if @error && @objects.empty?
%>

<%= paginate_block(@pages) do %>
  <%= render(partial: "shared/matrix_grid",
             locals: { objects: @objects }) %>
<% end %>

<!--NAME AUTOCOMPLETE INSTANCE FOR REUSE IN NAMING MODALS-->
<%= render(partial: "shared/form_name_autocomplete") %>
