<%
  @title ||= :create_name_title.t

  new_tab_set do
    add_tab(:all_objects.ths(:type => :name), :action => 'list_names')
  end
%>

<%= form_for(:name, :url => {
               :action => :create_name,
               :params => query_params,
            }) do |form| %>
  <br/><center><%= submit_tag(:CREATE.l) %></center>

  <%= render(:partial => 'form_name', :locals => {:form => form }) %>

  <br/><center><%= submit_tag(:CREATE.l) %></center>
<% end %>
