<%
add_page_title(:show_rss_log_title.t(title: @rss_log.unique_format_name))
add_pager_for(@rss_log)
%>

<% if @rss_log.notes %>
  <table>
    <% @rss_log.parse_log.each do |key, args, time|
      if key != :log_orphan %>
        <tr>
          <td class="text-nowrap align-text-top">
	    <%= time.web_time %>
	  </td>
          <td style="width:10px">:</td>
          <td><%= key.t(args) %></td>
        </tr>
      <% end
    end %>
  </table>
<% end %>

<%= if target = @rss_log.target
  link_to(:show_object.t(type: target.type_tag), target.show_link_args)
end %>
