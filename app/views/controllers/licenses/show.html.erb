<%
  @container = :wide
  add_page_title(license_show_title(@license))
  add_tab_set(license_show_tabs(@license))
%>

<div>
  <p><b>License id:</b> <%= @license.id %></p>
  <p><b>Updated_at:</b> <%= license_updated_at(@license) %></p>
  <p><b>Deprecated:</b> <%= @license.deprecated.to_s %></p>
  <p><b>Name:</b> <%= @license.display_name %></p>
  <p><b><%= :license_code.t %>:</b> <%= @license.code %></p>
  <p><b>url:</b> <%=
    link_to(@license.url, @license.url,
            { target: "_blank", rel: "noopener noreferrer" })
  %>
  </p>
</div>
