<%=
autocompleter = case filter.sym.to_s
                when "region"
                  "location"
                else
                  filter.sym.to_s
                end
between = tag.span(:"advanced_search_filter_#{filter.sym}".t,
                   class: "help-note mr-3")
autocompleter_field(
  form: f, field: filter.sym, type: autocompleter, label: "#{filter.name.t}:",
  between: between, value: @filter_defaults[filter.sym], separator: " OR "
)
%>
