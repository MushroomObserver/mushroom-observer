<%
@container = :wide
add_index_title(@query)
add_context_nav(articles_index_tabs(user: @user))
add_sorter(@query, articles_index_sorts)

flash_error(@error) if @error && @objects.empty?
%>

<%= pagination_nav_block(@pagination_data) do %>

  <div class="list-group">
    <%= render(partial: "articles/object", collection: @objects) %>
  </div>
<% end %>
