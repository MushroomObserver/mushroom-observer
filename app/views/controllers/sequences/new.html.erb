<%
container_class(:full)
add_new_title(:add_object, :SEQUENCE)
add_context_nav(sequence_form_tabs(obj: @observation))
%>

<div class="row">
  <div class="col-xs-12 col-sm-7">
    <%= render(partial: "sequences/observation_title",
               locals: { observation: @observation }) %>
    <%= render(partial: "sequences/form", locals: { local: true }) %>
  </div>

  <div class="col-xs-12 col-sm-5">
    <ul class="row list-unstyled">
      <%= render(partial: "shared/matrix_box",
                 locals: { object: @observation.rss_log || @observation,
                           columns: "col-xs-12" }) %>
    </ul>
  </div>
</div>
<!--.row-->
