<%
@container = :double
add_page_title(:users_by_contribution_title.t)
add_tab_set(contributors_index_tabs)
%>

<%= paginate_block(@pages) do %>
  <div class="row">
    <div class="col-md-6">
      <%= render(partial: "contributors/legend") %>
    </div>
  </div><!--.row-->

  <%= matrix_table(partial: "contributors/contributor",
                   objects: @objects, as: :user) %>
<% end %>
