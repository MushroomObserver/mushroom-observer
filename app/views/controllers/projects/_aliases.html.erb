<% project_id = project.id %>
<%= tag.div(id: "user_project_alias_#{user.id}") do %>
  <% project.alias_data(user).each do |name, id| %>
    <%= edit_project_alias_link(project_id, name, id) %><br/>
  <% end %>
  <% if project.alias_data(user).any? %>
    <br/>
  <% end %>
  <%= new_project_alias_link(project_id, user.id, User) %><br/>
<% end %>
