<% project_id = project.id %>
<%= tag.div(id: "target_project_alias_#{target.id}") do %>
  <% project.alias_data(target).each do |name, id| %>
    <%= edit_project_alias_link(project_id, name, id) %>
    <%= destroy_button(target: project_alias_path(project_id:, id:),
                       icon: :delete) %>
    <br/>
  <% end %>
  <% if project.alias_data(target).any? %>
    <br/>
  <% end %>
  <%= new_project_alias_link(project_id, target.id, target.class) %><br/>
<% end %>
