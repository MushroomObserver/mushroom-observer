<%
  add_page_title(:FIELD_SLIPS.t)
  add_tab_set(field_slips_index_tabs)
%>

<% if notice %>
  <p class="alert alert-success"><%= notice %></p>
<% end %>

<div id="field_slips">
  <% @field_slips.each do |field_slip| %>
    <strong><%= :field_slip_code.t %>:</strong>
    <%= link_to field_slip.code, field_slip_path(field_slip) %>
    <%= render field_slip %>
    <hr/>
  <% end %>
</div>
