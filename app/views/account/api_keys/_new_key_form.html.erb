<%= form_with(model: @key, url: account_api_keys_path, method: :post,
              remote: true, id: "account_new_api_key_form") do |f| %>

  <%= text_field_with_label(form: f, field: :notes,
                            label: "#{:account_api_keys_notes_label.t}",
                            class: "mt-3") %>

  <%= submit_button(form: f, button: :account_api_keys_create_button.l,
                    center: true, id: "create_button") %>

<% end %>
