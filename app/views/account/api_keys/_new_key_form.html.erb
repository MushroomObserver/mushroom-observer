<%= form_with(scope: :key, url: account_api_keys_path, method: :post,
              id: "account_new_api_key_form") do |f| %>

  <div class="form-group mt-3">
    <%= f.label(:notes, :account_api_keys_notes_label.t) %>
    <%= f.text_field(:notes, class: "form-control") %>
  </div>

  <%= centered_submit(form: f, button: :account_api_keys_create_button.l,
                      id: "create_button") %>

<% end %>
