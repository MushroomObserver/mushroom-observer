// create, update, activate, remove:
// re-render the table #account_api_keys_table
// render flash messages in #page_flash

function updateApiKeysTable() {
  const table = document.getElementById("account_api_keys_table"),
  page_flash = document.getElementById("page_flash"),
  new_table_html = '<%= j render(partial: "account/api_keys/table") %>',
  new_flash_html = "<%= j flash_notices_html %>";

  table.outerHTML = new_table_html;
  page_flash.innerHTML = new_flash_html;
}

updateApiKeysTable();
