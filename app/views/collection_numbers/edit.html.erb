<%
@container = :full
add_page_title(
  :edit_collection_number_title.l(name: @collection_number.format_name)
)
add_tab_set(
  collection_number_form_edit_tabs(back: @back, c_n: @collection_number,
                                    obj: @back_object)
)
%>

<div class="row">
  <div class="col-xs-12 col-sm-7">
    <%= render(partial: "form", locals: { action: :update, local: true }) %>
  </div>

  <div class="col-xs-12 col-sm-5">
    <ul class="row list-unstyled">
      <% @collection_number.observations.each do |obs| %>
        <%= render(partial: "shared/matrix_box",
                   locals: { object: obs.rss_log || obs,
                             columns: "col-xs-12" }) %>
      <% end %>
    </ul>
  </div>
</div><!--.row-->
