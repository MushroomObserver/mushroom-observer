<% @title = :change_banner_title.t %>

<%= form_with(url: admin_banner_path, method: :put,
              id: "admin_banner_form") do |f| %>

  <%= f.text_area(:val, value: @val, rows: 5, id: "val",
                  class: "form-control") %>

  <%= f.submit(:SUBMIT.l, class: "btn btn-default center-block mt-3") %>

<% end %>
