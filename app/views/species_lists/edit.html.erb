<%
  @title = :species_list_edit_title.t(name: @species_list.unique_format_name)

  tabs = create_tabs(species_list_form_edit_links(list: @species_list))
  @tabsets = { right: draw_tab_set(tabs) }
  @container = :text

  action = species_list_path(@species_list, approved_where: @place_name)
%>

<%= render(partial: "species_lists/form",
           locals: { button: :UPDATE, action: action }) %>
