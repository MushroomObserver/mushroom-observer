<%
  @title = :species_list_upload_title.t
%>

<%= form_with(url: species_list_uploads_path(@species_list.id),
              multipart: true) do |f| %>

  <div class="form-group mt-3">
    <%= :species_list_upload_label.t %>:
    <%= custom_file_field(:species_list, :file) %>
    <div class="help_block">
      <%= :species_list_upload_help.tp %>
    </div>
  </div>

  <%= f.submit(:UPLOAD.l, class: "btn btn-default center-block mt-3") %>

<% end %>
