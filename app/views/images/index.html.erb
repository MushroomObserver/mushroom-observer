<%
  if @links.any?
    tabs = create_links(@links)
    @tabsets = { right: draw_tab_set(tabs) }
  end

  @container = :full
  flash_error(@error) if @error && @objects.empty?
%>

<%= paginate_block(@pages) do %>
  <%= render(partial: "shared/matrix_box",
             layout: "shared/matrix_grid",
             collection: @objects,
             as: :object) %>
<% end %>
